//*변수의 교환법칙 + 

package finalnotepackage;

public class VariableExchange {
	public static void main(String[] args) {
		int num1 = 200;
		int num2 = 400;
		int temp;
		System.out.print("num1:"+num1+" num2:"+num2);
		System.out.println();
		temp = num1;
		num1 = num2;
		num2 = temp;
		System.out.print("num1:"+num1+" num2:"+num2);
	}
}

//*괄호
-먼저 처리할 쪽을 먼저 괄호로 묶는다.

package finalnotepackage;

public class Bracketnote {
	public static void main(String[] args) {
		int sum = 2+5*3;
		System.out.println(+sum);
		sum = (2+4)*4;
	}
}
