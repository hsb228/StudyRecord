//변수의 교환법칙

package finalnotepackage;

public class VariableExchange {
	public static void main(String[] args) {
		int num1 = 200;
		int num2 = 400;
		int temp;
		System.out.print("num1:"+num1+" num2:"+num2);
		System.out.println();
		temp = num1;
		num1 = num2;
		num2 = temp;
		System.out.print("num1:"+num1+" num2:"+num2);
	}
}
